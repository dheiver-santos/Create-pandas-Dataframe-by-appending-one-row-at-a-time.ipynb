      "source": [
        ">>> import pandas as pd\n",
        ">>> from numpy.random import randint\n",
        "\n",
        ">>> df = pd.DataFrame(columns=['lib', 'qty1', 'qty2'])\n",
        ">>> for i in range(5):\n",
        ">>>     df.loc[i] = ['name' + str(i)] + list(randint(10, size=2))\n",
        "\n",
        ">>> df"
